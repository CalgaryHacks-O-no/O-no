{
	"name": "frontend",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.30",
		"@fortawesome/free-solid-svg-icons": "^5.14.0",
		"@fortawesome/react-fontawesome": "^0.1.11",
		"@testing-library/react": "^9.5.0",
		"@testing-library/user-event": "^7.2.1",
		"axios": "^0.19.2",
		"marked": "^1.1.1",
		"re-resizable": "^6.5.5",
		"react": "^16.13.1",
		"react-beautiful-dnd": "^13.0.0",
		"react-burger-menu": "^2.7.1",
		"react-dom": "^16.13.1",
		"react-dropzone": "^11.0.3",
		"react-router-dom": "^5.2.0",
		"react-scripts": "^3.4.3",
		"react-simplemde-editor": "^4.1.3",
		"react-tiny-popover": "^6.0.0-beta.2",
		"regenerator-runtime": "^0.13.7",
		"sanitize-html": "^1.27.4"
	},
	"scripts": {
		"old-start": "webpack --mode development ./view/src/index.js --output ./view/static/main.js --watch",
		"prod": "webpack --mode production ./view/src/index.js --output ./view/static/main.js",
		"start": "webpack --watch",
		"build": "react-scripts build",
		"test": "jest",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@babel/preset-env": "^7.11.5",
		"@babel/preset-react": "^7.10.4",
		"@testing-library/jest-dom": "^4.2.4",
		"babel-jest": "^26.3.0",
		"babel-loader": "^8.1.0",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"enzyme": "^3.11.0",
		"jest": "^26.4.2",
		"url-loader": "^4.1.0",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	},
	"jest": {
		"setupFilesAfterEnv": [
			"./view/src/setupTests.js"
		],
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/view/src/__mocks__/fileMock.js",
			"\\.(css|less)$": "<rootDir>/view/src/__mocks__/styleMock.js",
			"^~(.*)": "<rootDir>/view/src$1"
		},
		"testPathIgnorePatterns": [
			"/node_modules/",
			"<rootDir>/view/src/__tests__/__utils__/"
		]
	}
}
